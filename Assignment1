#include <iostream>
using namespace std;

class car{
public:

int release_year;
string customer;
int speed;

public:
void printinfo(void);
int accelarate(int);
int brake(int);

};

void car :: printinfo(){
    cout<<"relyear=";
    cout<<release_year<<endl;
    cout<<"customer=";
    cout<<customer<<endl;
    cout<<"speed=";
    cout<<speed<<endl;
    };
int car :: accelarate(int speed){
    for(int i=0;i<5;i++)
    {
        speed=speed+10;
    }
    cout<<"The Accelerated speed is:"<<speed<<endl;
}

int car :: brake(int speed){
    for(int i=0;i<5;i++)
    {
        if(speed>0){
        speed=speed-10;}
    }
    cout<<"The Decelerated speed is:"<<speed<<endl;
}

int main() {
    int n;
    cout<<"enter no of inputs: ";
    cin>>n;
    car arr[n];
    for(int i=0;i<n;i++){
        cout<<"relyear=";
        cin>>arr[i].release_year;
        cout<<"customer=";
        cin>>arr[i].customer;
        cout<<"speed=";
        cin>>arr[i].speed;
    };
    
    for(int i=0;i<n;i++){
    arr[i].printinfo();
    arr[i].accelarate(arr[i].speed);
    arr[i].brake(arr[i].speed);
    }

}
